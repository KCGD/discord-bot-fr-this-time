{
  "name": "hodorsascension",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "npx tsc && echo \"--- START PROGRAM ---\" && node src/main",
    "build": "npx tsc && npx pkg package.json -C Brotli",
    "clean": "find ./src -name \"*.js\" -type f | while read file; do rm -v \"$file\"; done",
    "package": "npm i && npx npm run build && npm run clean"
  },
  "author": "",
  "license": "GPL-2.0-only",
  "devDependencies": {
    "@types/node": "^18.16.0",
    "pkg": "^5.8.1",
    "typescript": "^5.0.4"
  },
  "pkg": {
    "outputPath": "./Build",
    "targets": [
      "node18"
    ],
    "assets": [
      "./src/**/*"
    ]
  },
  "bin": "./src/main.js",
  "dependencies": {
    "@discordjs/builders": "^1.6.3",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.7.1",
    "@discordjs/voice": "^0.16.0",
    "@types/cli-color": "^2.0.2",
    "cli-color": "^2.0.3",
    "discord.js": "^14.11.0",
    "tweetnacl": "^1.0.3",
    "ws": "^8.13.0"
  }
}
